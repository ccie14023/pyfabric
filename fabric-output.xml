<router>
  <lisp>
    <encapsulation>
      <vxlan/>
    </encapsulation>
    <locator-table>
      <default/>
    </locator-table>
    <locator-set>
      <name>rloc1</name>
      <IPv4-interface>
        <name>Loopback0</name>
        <priority>10</priority>
        <weight>10</weight>
      </IPv4-interface>
    </locator-set>
    <eid-table>
      <instance-list>
        <instance-id>0</instance-id>
        <default/>
      </instance-list>
      
    </eid-table>
    <disable-ttl-propagate/>
    <ipv4>
      <sgt/>
      <use-petr>
        <locator-address>192.168.110.1</locator-address>
      </use-petr>
      <itr>
        <map-resolver>
          <ip-addr>192.168.110.1</ip-addr>
        </map-resolver>
      </itr>
      <itr-enable>
        <itr/>
      </itr-enable>
      <etr>
        <map-server>
          <ip-addr>192.168.110.1</ip-addr>
        <key>
          <key-0>uci</key-0>
        </key>
        </map-server>
      </etr>
      <etr-enable>
        <etr/>
      </etr-enable>
    </ipv4>
  </lisp></router>
